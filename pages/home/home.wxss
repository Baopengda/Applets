.les {
  background-color: red;
  width: 200rpx;
  height: 200rpx;
}
swiper {
  height: calc(400*100vw/750);
}
swiper image {
  width: 100vw;
  height: calc(400*100vw/750);
}
.custom-class {
  border-radius: 5rpx;
}
.brand {
  width: 100%;
  background-color: white;
  margin: 20rpx 0rpx;
}
.brand .brand_top {
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  margin-top: 40rpx;
}
.brand .van-grid-item__content {
  padding: 2rpx;
}
.brand .brand_list {
  position: relative;
}
.brand .brand_list .brand_list_text1 {
  position: absolute;
  top: 20rpx;
  left: 40rpx;
  font-size: 28rpx;
}
.brand .brand_list .brand_list_text2 {
  position: absolute;
  top: 60rpx;
  left: 40rpx;
  font-size: 24rpx;
  color: brown;
}
.common_components .common_components_image {
  width: 100%;
  height: 200rpx;
}
.common_components .common_components_text1 {
  margin: 10rpx 0;
  font-size: 24rpx;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.common_components .common_components_text2 {
  margin: 10rpx 0;
  font-size: 24rpx;
  text-align: center;
}
.swiperss {
  width: 100%;
  height: 550rpx;
}
.swiperss .swiperss_cart .swiperss_cart_text1 {
